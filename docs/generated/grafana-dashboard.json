{
  "dashboard": {
    "id": null,
    "title": "Zephix Frontend Monitoring",
    "tags": ["zephix", "frontend", "react"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"zephix-frontend\"}",
            "legendFormat": "Frontend Health"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"zephix-frontend\"}[5m]))",
            "legendFormat": "P95 Response Time"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"zephix-frontend\"}[5m]))",
            "legendFormat": "P50 Response Time"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"zephix-frontend\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors"
          },
          {
            "expr": "rate(http_requests_total{job=\"zephix-frontend\",status=~\"4..\"}[5m])",
            "legendFormat": "4xx Errors"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "Request Volume",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"zephix-frontend\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Build Information",
        "type": "table",
        "targets": [
          {
            "expr": "zephix_build_info",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "build_tag": "Build Tag",
                "git_hash": "Git Hash",
                "build_time": "Build Time",
                "environment": "Environment"
              }
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "API Client Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(zephix_api_requests_total[5m])",
            "legendFormat": "API Requests/sec"
          },
          {
            "expr": "rate(zephix_api_requests_total{status=\"success\"}[5m])",
            "legendFormat": "Successful API Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Authentication Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(zephix_auth_requests_total[5m])",
            "legendFormat": "Auth Requests/sec"
          },
          {
            "expr": "rate(zephix_auth_requests_total{status=\"success\"}[5m])",
            "legendFormat": "Successful Auth/sec"
          },
          {
            "expr": "rate(zephix_token_refresh_total[5m])",
            "legendFormat": "Token Refreshes/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}
