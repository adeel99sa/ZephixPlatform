{
  "name": "@zephix/user-auth-service",
  "version": "1.0.0",
  "description": "Enterprise user authentication service for Zephix platform",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon src/main.js",
    "test:register": "node test-registration.js",
    "setup:uuid": "node setup-uuid.js",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:integration": "jest --config ./tests/jest-integration.json",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "security:deps": "audit-ci --config audit-ci.json",
    "clean": "rm -rf dist coverage",

    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",

    "db:setup": "ts-node scripts/setup-database.ts",
    "db:migrate": "ts-node scripts/migrate.ts",
    "db:migrate:generate": "ts-node scripts/migrate-generate.ts",
    "db:migrate:revert": "ts-node scripts/migrate-revert.ts",
    "db:seed": "ts-node scripts/seed.ts",
    "db:seed:test": "ts-node scripts/seed-test.ts",
    "db:reset": "ts-node scripts/reset-database.ts",

    "migrate": "ts-node scripts/migrate.ts",
    "migrate:generate": "ts-node scripts/migrate-generate.ts",
    "migrate:revert": "ts-node scripts/migrate-revert.ts",
    "seed": "ts-node scripts/seed.ts",
    "seed:test": "ts-node scripts/seed-test.ts",

    "docker:build": "docker build -f docker/Dockerfile -t zephix/user-auth-service .",
    "docker:build:dev": "docker build -f docker/Dockerfile.dev -t zephix/user-auth-service:dev ."
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "2.4.3",
    "uuid": "^9.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "express-prom-bundle": "6.6.0",
    "express-rate-limit": "6.10.0",
    "express-validator": "7.0.1",
    "express-winston": "4.2.0",
    "helmet": "7.0.0",
    "joi": "17.9.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "node-cron": "3.0.2",
    "nodemailer": "6.9.4",
    "pg": "8.11.3",
    "prom-client": "14.2.0",
    "qrcode": "1.5.3",
    "redis": "4.6.7",
    "reflect-metadata": "0.1.13",
    "speakeasy": "2.0.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "twilio": "4.10.0",
    "typeorm": "0.3.17",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.5",
    "@types/nodemailer": "6.4.9",
    "@types/pg": "8.10.2",
    "@types/qrcode": "1.5.2",
    "@types/speakeasy": "2.0.7",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "audit-ci": "6.6.1",
    "eslint": "8.45.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zephix/platform.git"
  },
  "keywords": [
    "authentication",
    "jwt",
    "oauth",
    "security",
    "microservice"
  ],
  "author": "Zephix Team",
  "license": "MIT"
}
