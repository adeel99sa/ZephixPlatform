{
  "name": "zephix-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "npm run build:metadata",
    "build": "nest build",
    "build:metadata": "echo \"export const BUILD_METADATA = { sha: '$(git rev-parse HEAD 2>/dev/null || echo \"unknown\")', timestamp: '$(date -u +\"%Y-%m-%dT%H:%M:%S.%3NZ\")' };\" > src/build-metadata.ts",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "start:railway": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:local": "cross-env NODE_ENV=development nest start --watch",
    "db:local:reset": "rm -f dev.db && npm run start:local",
    "db:local:seed": "ts-node -r tsconfig-paths/register scripts/seed-local-db.ts",
    "test:local": "NODE_ENV=test jest --config jest-local.config.js",
    "start:debug": "nest start --debug --watch",
    "dev": "ts-node src/main.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:email": "node scripts/test-email.js",
    "check:user": "node scripts/check-user.js",
    "verify:email": "node scripts/verify-user-email.js",
    "check:email-config": "node scripts/check-email-config.js",
    "test:login": "node scripts/test-user-login.js",
    "test:auth:direct": "node scripts/test-user-auth-direct.js",
    "migration:run": "ts-node -r tsconfig-paths/register scripts/run-migrations-production.ts",
    "migration:run:dev": "ts-node -r tsconfig-paths/register scripts/run-migrations.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "migrate:generate": "ts-node -r tsconfig-paths/register scripts/migrate-generate.ts",
    "db:migrate": "npm run migration:run",
    "db:migrate:dev": "npm run migration:run:dev",
    "db:migrate:generate": "npm run migrate:generate",
    "postbuild": "echo 'Build completed successfully'",
    "health:check": "curl -f http://localhost:${PORT:-3000}/api/health || exit 1",
    "verify:railway": "./scripts/verify-railway-build.sh",
    "validate:deployment": "ts-node scripts/validate-deployment.ts",
    "dep:cycles": "tsc -p tsconfig.build.json && madge dist --circular",
    "test:guards": "npm test -- --testPathPattern='(organization|project-permission).guard.spec.ts' --verbose",
    "auth:diagnose": "ts-node scripts/auth-diagnostics.ts",
    "auth:diagnose:browser": "node scripts/browser-auth-diagnostics.js",
    "auth:diagnose:full": "chmod +x scripts/run-auth-diagnostics.sh && ./scripts/run-auth-diagnostics.sh",
    "auth:test": "npm run auth:diagnose && npm run auth:diagnose:browser"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/instrumentation-express": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.49.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/nodemailer": "^6.4.17",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "mammoth": "^1.10.0",
    "multer": "^2.0.2",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^7.0.5",
    "openai": "^4.28.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pino": "^9.8.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "madge": "^8.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
