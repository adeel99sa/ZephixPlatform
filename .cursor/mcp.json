{
  "mcpServers": {
    "railway-mcp": {
      "command": "npx",
      "args": ["-y", "@railway/mcp-server"],
      "env": {
        "RAILWAY_TOKEN": "${env:RAILWAY_TOKEN}"
      }
    },
    "filesystem-mcp": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem"],
      "env": {
        "MCP_FILESYSTEM_ROOT": "${workspaceFolder}"
      }
    }
  }
}
