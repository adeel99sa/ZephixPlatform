  "automationRules": [
    {
      "id": "rule_001",
      "name": "Sync Jira to GitHub Issues",
      "source": "Jira",
      "target": "GitHub",
      "trigger": "issue.created",
      "conditions": ["issue.type = Bug", "issue.priority >= High"],
      "actions": ["create GitHub issue", "add label", "assign to team"],
      "enabled": true
    },
    {
      "id": "rule_002",
      "name": "Notify Slack on High Risk",
      "source": "Zephix",
      "target": "Slack",
      "trigger": "risk.created",
      "conditions": ["risk.severity = High", "risk.probability >= 0.7"],
      "actions": ["post to #pm-alerts", "mention @pm-team"],
      "enabled": true
    },
    {
      "id": "rule_003",
      "name": "Update Azure DevOps on Deploy",
      "source": "GitHub",
      "target": "Azure DevOps",
      "trigger": "deployment.success",
      "conditions": ["branch = main", "environment = production"],
      "actions": ["update work item", "move to Done", "add comment"],
      "enabled": true
    }
  ]
