{
  "name": "zephix-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port ${PORT:-3000}",
    "start": "serve dist -l ${PORT:-3000}",
    "serve": "serve dist -l ${PORT:-3000}",
    "lint": "eslint .",
    "prebuild": "rm -rf dist/*",
    "test": "vitest",
